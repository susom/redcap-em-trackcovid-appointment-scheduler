{
  "name": "kigkonsult/icalcreator",
  "type": "library",
  "description": "iCalcreator is the PHP implementation of rfc2445/rfc5545, management of calendar information",
  "keywords": [
    "rfc2445",
    "rfc5545",
    "rfc6321",
    "management",
    "calendar",
    "vCalendar",
    "event",
    "todo",
    "alarm",
    "vevent",
    "vtodo",
    "vjournal",
    "vfreebusy",
    "valarm",
    "vtimezone",
    "standard",
    "daylight"
  ],
  "license": "lgpl-3.0",
  "support": {
    "issues": "https://github.com/iCalcreator/iCalcreator/issues"
  },
  "authors": [
    {
      "name": "Kjell-Inge Gustafsson",
      "email": "ical@kigkonsult.se"
    }
  ],
  "autoload": {
    "files": [
      "autoload.php"
    ],
    "psr-4": {
      "Kigkonsult\\Icalcreator\\": [
        "src/",
        "src/Util/",
        "src/Traits/"
      ]
    }
  },
  "require": {
    "php": ">=5.6",
    "ext-simplexml": "*",
    "ext-openssl": "*",
    "ext-zlib": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "6.5.13"
  }
}
